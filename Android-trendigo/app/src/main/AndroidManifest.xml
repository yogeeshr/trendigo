<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.inmobi.com.trendigo" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="android.inmobi.com.trendigo.HomePage"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.SearchEvent"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.SearchRestaurant"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.VegSearch"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.NonVegSearch"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.StarOutletSearch"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.PubSearch"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.ApiRequest"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="android.inmobi.com.trendigo.RestaurantSearchResult"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="android.inmobi.com.trendigo.EventSearchResult"
            android:label="@string/app_name">
        </activity>

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDyiNoDWj3y0Z7nqvQjbQu9yYRxx_AUdAM" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

</manifest>
